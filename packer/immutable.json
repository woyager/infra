{
	"variables": {
		"proj_id": null,
		"source_image": null,
		"machine_type": "f1-micro"
	},
	"builders": [
		{
		"type": "googlecompute",
		"project_id": "{{user `proj_id`}}",
		"image_name": "reddit-bake-{{timestamp}}",
		"source_image": "{{user `source_image`}}",
		"zone": "europe-west1-b",
		"ssh_username": "appuser",
		"machine_type": "{{user `machine_type`}}",
		"disk_size": "10",
		"disk_type": "pd-standard",
		"image_description": "reddit-bake",
		"network": "default",
		"tags":	[
				"puma-server"
			],
		"ssh_timeout": "120s"
		}
	],
	"provisioners": [
		{
		"type": "shell",
		"script": "files/bake.sh",
		"execute_command": "sudo {{.Path}}"
		}
	]
}
